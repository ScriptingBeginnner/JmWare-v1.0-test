local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("JmWare", "Synapse")
local Tab = Window:NewTab("Main")
local Section = Tab:NewSection("JmWare v1.0")
local Section = Tab:NewSection("If you encounter any bugs dm me")
local Section = Tab:NewSection("Midsko#4686")
local Section = Tab:NewSection("Remember, this is the first version, Enjoy!")

local Tab = Window:NewTab("Animations")
local Section = Tab:NewSection("Animations")
Section:NewButton("Pro Animations", "Pro Animations", function()
    while true do
        local Animate = game.Players.LocalPlayer.Character.Animate
        Animate.idle.Animation1.AnimationId = "http://www.roblox.com/asset/?id=782841498"
        Animate.idle.Animation2.AnimationId = "http://www.roblox.com/asset/?id=782841498"
        Animate.walk.WalkAnim.AnimationId = "http://www.roblox.com/asset/?id=616168032"
        Animate.run.RunAnim.AnimationId = "http://www.roblox.com/asset/?id=616163682"
        Animate.jump.JumpAnim.AnimationId = "http://www.roblox.com/asset/?id=656117878"
        Animate.climb.ClimbAnim.AnimationId = "http://www.roblox.com/asset/?id=1083439238"
        Animate.fall.FallAnim.AnimationId = "http://www.roblox.com/asset/?id=656115606"
        game.Players.LocalPlayer.Character.Humanoid.Jump = false
        wait(1)
        end
    end)

local Tab = Window:NewTab("Others")
local Section = Tab:NewSection(":D")
Section:NewButton("Fly", "Fly Script", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/RobloxPlayerGuy/RobloxFlyGui/main/Script"))()
end)

local Tab = Window::NewTab("keybinds")
local Section = Tab:NewSection("The motherfucking keybinds")
Section:NewKeybind("ToggleUI", "ToggleUI", Enum.KeyCode.V, function()
    Library:ToggleUI()
end)
